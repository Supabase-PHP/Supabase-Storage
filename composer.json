{
    "name": "supabase-php/supabase-storage",
    "description": "Supabase storage for php client",
    "type": "library",
    "require-dev": {
        "pestphp/pest": "^3.5",
        "phpstan/phpstan": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Supabase\\Storage\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Sushil Kumar",
            "email": "sushilkumbhar77@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
      "php": "^8.1",
        "guzzlehttp/guzzle": "^7.9",
        "vlucas/phpdotenv": "^5.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
